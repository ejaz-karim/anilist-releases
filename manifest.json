{
  "manifest_version": 3,
  "name": "anilist-releases",
  "version": "0.1",
  "description": "",
  "permissions": [
    "activeTab"
  ],
  "host_permissions": [
    "https://api.ani.zip/*",
    "https://zenshin-supabase-api.onrender.com/*",
    "https://feed.animetosho.org/*",
    "https://nyaa.si/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://anilist.co/*"
      ],
      "js": [
        "dist/index.js"
      ],
      "run_at": "document_idle"
    }
  ]
}