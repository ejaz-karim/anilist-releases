{
  "manifest_version": 3,
  "name": "anilist-releases",
  "version": "0.1",
  "description": "",
  "permissions": [
    "activeTab",
    "scripting"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://anilist.co/*"
      ],
      "js": [
        "dist/index.js"
      ],
      "run_at": "document_idle"
    }
  ]
}