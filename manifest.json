{
  "manifest_version": 3,
  "name": "AniList Releases",
  "version": "1.0.0",
  "description": "Seadex and Nyaa integration to AniList entries.",
  "permissions": [],
  "icons": {
    "128": "icon.png"
  },
  "host_permissions": [
    "https://releases.moe/*",
    "https://api.ani.zip/*",
    "https://zenshin-supabase-api.onrender.com/*",
    "https://feed.animetosho.org/*",
    "https://nyaa.si/*"
  ],
  "background": {
    "scripts": [
      "dist/background.js"
    ],
    "service_worker": "dist/background.js"
  },
  "content_scripts": [
    {
      "matches": [
        "*://anilist.co/*"
      ],
      "js": [
        "dist/index.js"
      ],
      "run_at": "document_idle"
    }
  ],
  "browser_specific_settings": {
    "gecko": {
      "id": "{69d2101d-157e-4136-9cf4-4880f53ce95d}"
    }
  }
}